module mux_2NtoN
	# (parameter M = 2)
	  (I0, I1, S, O);
	  
	input wire [M-1:0] I0;
	input wire [M-1:0] I1;
	input wire          S;
	
	output reg [M-1:0]  O;
	
	always_ff @ (posedge clk) begin
	
		for (int i = 0; i < N; i++) begin
			
			O[i] = (I0[i] & ~S) | (I1[i] & S);
			
		end
	
	end 
	  
endmodule
